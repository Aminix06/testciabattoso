
if getgenv().ScriptAlreadyRunning then
	warn("Lo script √® gi√† in esecuzione! Ignoro questa esecuzione.")
	return
end

getgenv().ScriptAlreadyRunning = true

--==============================================================================
-- CONTROLLO SERVER VUOTO (Esecuzione Immediata e Continua)
--==============================================================================
task.spawn(function()
	repeat task.wait() until game:IsLoaded()
	
	local HttpService = game:GetService("HttpService")
	local TeleportService = game:GetService("TeleportService")
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	
	-- Verifica API per vedere se il server √® Pubblico (metodo sicuro per Executor)
	local function IsPublicServer()
		local jobId = game.JobId
		local placeId = game.PlaceId
		local cursor = ""
		local isPublic = false
		
		-- Loop attraverso la lista dei server pubblici
		while true do
			local url = "https://games.roblox.com/v1/games/" .. placeId .. "/servers/Public?limit=100&cursor=" .. cursor
			local requestFunc = (syn and syn.request) or (http and http.request) or http_request or request
			
			if not requestFunc then
				warn("Executor non supporta request. Impossibile verificare server pubblico.")
				return true -- Nel dubbio assumiamo pubblico per sicurezza
			end

			local success, response = pcall(function()
				return requestFunc({Url = url, Method = "GET"})
			end)

			if not success or not response or response.StatusCode ~= 200 then
				task.wait(1) -- Riprova se fallisce
			else
				local data = HttpService:JSONDecode(response.Body)
				for _, server in ipairs(data.data) do
					if server.id == jobId then
						return true -- Trovato nella lista pubblica!
					end
				end
				cursor = data.nextPageCursor
				if not cursor then break end
			end
			task.wait(0.1)
		end
		return false -- Non trovato nella lista pubblica -> √à PRIVATO
	end

	print("üîç Analisi tipo di server in corso...")
	local publico = IsPublicServer()

	if publico then
		-- CASO 1: SEI IN PUBBLICA -> KICK
		LocalPlayer:Kick("SEI IN UN SERVER PUBBLICO! Kick di sicurezza attivato.")
	else
		-- CASO 2: SEI IN PRIVATA
		print("üîí Server Privato confermato.")
		
		-- Controlliamo se ci sono intrusi
		local function CheckIntruders()
			if #Players:GetPlayers() > 1 then
				warn("‚ö†Ô∏è Intruso rilevato nel server privato! Cambio mondo...")
				
				-- Funzione Queue per ricaricare lo script dopo il TP
				local queue_on_teleport = (syn and syn.queue_on_teleport) or queue_on_teleport or (fluxus and fluxus.queue_on_teleport)
				if queue_on_teleport then
					queue_on_teleport([[
						repeat task.wait() until game:IsLoaded()
						loadstring(game:HttpGet("https://raw.githubusercontent.com/Aminix06/testciabattoso/refs/heads/main/autociabattoso"))()
					]])
				end

				-- Logica TP Vills <-> Terra
				local TPEvent = ReplicatedStorage:WaitForChild("Package"):WaitForChild("Events"):WaitForChild("TP")
				if game.PlaceId == 3311165597 then -- Terra
					TPEvent:InvokeServer("Vills Planet")
				elseif game.PlaceId == 5151400895 then -- Vills
					TPEvent:InvokeServer("Earth")
				end
			end
		end

		-- Controllo iniziale
		CheckIntruders()

		-- Controllo continuo (se entra qualcuno dopo)
		Players.PlayerAdded:Connect(function()
			CheckIntruders()
		end)
	end
end)

local function CheckToGo()
	local player = game.Players.LocalPlayer
	local humanoidRootPart = player.Character:WaitForChild("HumanoidRootPart")
	local playerId = player.UserId
	local data = game:GetService("ReplicatedStorage").Datas
	local playerData = data:WaitForChild(tostring(playerId))
	local statsz = playerData:WaitForChild("Strength")
	local zeni = playerData:WaitForChild("Zeni")
	while true do
		if statsz.Value < 100000000 and game.PlaceId == 5151400895 then
			local args = {
				[1] = "Earth"
			}
 
			game:GetService("ReplicatedStorage").Package.Events.TP:InvokeServer(unpack(args))
		end
		wait(.1)
	end
end
task.spawn(CheckToGo)
 
 
local firstspawn = true
 
local function startonskibidi()
	if firstspawn and game.PlaceId == 5151400895 then
		print("Start")
		firstspawn = false
	else
		print("died")
	end
end
 
game.Players.LocalPlayer.CharacterAdded:Connect(startonskibidi)
 
 
local function StartTheGame()
    -- FIX: Attendi che la GUI 'Main' sia disponibile prima di interagire con essa.
    local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    local mainGui = playerGui:WaitForChild("Main", 10) -- Attendi fino a 10 secondi

    if not mainGui then
        warn("StartTheGame: GUI 'Main' non trovata. Impossibile continuare la funzione.")
        return
    end

	task.wait()
	local Event = game:GetService("ReplicatedStorage").Package.Events.Start
	Event:InvokeServer()
	task.wait()
 
	local Event = game:GetService("ReplicatedStorage").Package.Events.Start
	Event:InvokeServer()
 
	task.wait()
 
    local bruhButton = mainGui:FindFirstChild("bruh")
    if bruhButton then
	    bruhButton.Disabled = true
	    bruhButton.Disabled = false
    end
 
	task.wait()
	local Event = game:GetService("ReplicatedStorage").Package.Events.Start
	Event:InvokeServer()
end
 
StartTheGame()
 
wait(.5)
 
 
