--==============================================================================
-- WRAPPER DI SICUREZZA E CREAZIONE DELLO "ZAINO MAGICO"
--==============================================================================
if getgenv().CiabattosoScriptLoaded then print("[Sistema] Script di Ciabattoso già caricato.") return end
getgenv().CiabattosoScriptLoaded = true
getgenv().CiabattosoFramework = {}
local Framework = getgenv().CiabattosoFramework
print("[Sistema] Framework persistente di Ciabattoso creato.")

--==============================================================================
-- IMPOSTAZIONI E GESTIONE DELLO STATO
--==============================================================================
Framework.SettingsFileName = "ciabattoso_settings.json"
Framework.Settings = {
    AutofarmEnabled = false
}

function Framework.SaveSettings()
    local success, data = pcall(game:GetService("HttpService").JSONEncode, game:GetService("HttpService"), Framework.Settings)
    if success then
        writefile(Framework.SettingsFileName, data)
        print("[Impostazioni] Salvate:", data)
    end
end

function Framework.LoadSettings()
    if isfile(Framework.SettingsFileName) then
        local success, data = pcall(readfile, Framework.SettingsFileName)
        if success then
            local success2, decoded = pcall(game:GetService("HttpService").JSONDecode, game:GetService("HttpService"), data)
            if success2 then Framework.Settings = decoded; print("[Impostazioni] Caricate:", data) end
        end
    end
end

--==============================================================================
-- CONTROLLO SERVER E FUNZIONI DI BASE
--==============================================================================
local Players, localPlayer = game:GetService("Players"), game:GetService("Players").LocalPlayer
if #Players:GetPlayers() > 1 then localPlayer:Kick("Kick: Il server non è vuoto.") return end
task.spawn(function() while task.wait() do if #Players:GetPlayers() > 1 then localPlayer:Kick("Kick: Un altro giocatore è entrato.") break end end end)

local function StartTheGame()
    local playerGui = localPlayer:WaitForChild("PlayerGui"); local mainGui = playerGui:WaitForChild("Main", 10)
    if not mainGui then warn("StartTheGame: GUI 'Main' non trovata.") return end
    task.wait(); local Event = game:GetService("ReplicatedStorage").Package.Events.Start
    Event:InvokeServer(); task.wait(); Event:InvokeServer(); task.wait()
    local bruhButton = mainGui:FindFirstChild("bruh"); if bruhButton then bruhButton.Disabled = true; bruhButton.Disabled = false end
    task.wait(); Event:InvokeServer()
end
StartTheGame(); wait(0.5)

-- Variabili del Framework
Framework.player = localPlayer
Framework.playerData = game:GetService("ReplicatedStorage").Datas:WaitForChild(tostring(Framework.player.UserId))
Framework.statsz = Framework.playerData:WaitForChild("Strength")
Framework.defensestats = Framework.playerData:WaitForChild("Defense")
Framework.zeni = Framework.playerData:WaitForChild("Zeni")
Framework.Events = game:GetService("ReplicatedStorage"):WaitForChild("Package"):WaitForChild("Events")
Framework.ActiveThreads = {}

--==============================================================================
-- FUNZIONI GUI E FORMATTAZIONE
--==============================================================================
function Framework.formatNumber(number) if number>=1e12 then return string.format("%.1fT",number/1e12)elseif number>=1e9 then return string.format("%.1fB",number/1e9)elseif number>=1e6 then return string.format("%.1fM",number/1e6)elseif number>=1e3 then return string.format("%.1fK",number/1e3)else return tostring(math.floor(number))end end
function Framework.formatNumberForTheStats(number) if number>=1e12 then return string.format("%.2fT",number/1e12)elseif number>=1e9 then return string.format("%.2fB",number/1e9)elseif number>=1e6 then return string.format("%.2fM",number/1e6)elseif number>=1e3 then return string.format("%.2fK",number/1e3)else return tostring(number)end end

