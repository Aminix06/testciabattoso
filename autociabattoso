        {name = "SSJ2 Kaioken", forza = 50000},
        {name = "SSJ Kaioken", forza = 16000},
    }

    -- Funzione per trovare la cartella Status ovunque sia
    local function getStatusFolder()
        return yo:FindFirstChild("Status") or (yo.Character and yo.Character:FindFirstChild("Status"))
    end

    print("IL SOMMO: Script Avviato con successo!")

    while true do
        pcall(function()
            local strength = folderData.Strength.Value
            local status = getStatusFolder()
            
            if status then
                local selected = status:FindFirstChild("SelectedTransformation")
                local actual = status:FindFirstChild("Transformation")

                for _, v in ipairs(transformaciones) do
                    if strength >= v.forza then
                        -- Se non √® gi√† quella selezionata, proviamo a equipparla
                        if selected.Value ~= v.name then
                            equipRemote:InvokeServer(v.name)
                            task.wait(0.3) -- Tempo per il server di rispondere
                        end

                        -- Se l'equip ha funzionato (o era gi√† giusto)
                        if selected.Value == v.name then
                            -- Se non siamo trasformati, trasformiamoci!
                            if actual.Value ~= selected.Value then
                                -- Controllo Ki (opzionale, rimosso per massima velocit√†)
                                transformRemote:InvokeServer()
                            end
                            break -- Esci dal ciclo, abbiamo la forma migliore
                        end
                    end
                end
            else
                warn("IL SOMMO: Cartella 'Status' non trovata n√© nel Player n√© nel Character!")
            end
        end)
        task.wait(1.5)
    end
end)

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Events = ReplicatedStorage:WaitForChild("Package"):WaitForChild("Events")
 
local function SafeWaitForChild(parent, name, timeout)
	local success, result = pcall(function()
		return parent:WaitForChild(name, timeout)
	end)
	if not success then
		warn("Failed to find " .. name .. ": " .. tostring(result))
		return nil
	end
	return result
end
 
